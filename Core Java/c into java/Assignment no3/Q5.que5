package Assignmentno4;

public class que5 {

	public static void main(String[] args) {
		int num=153, original, rem, sum = 0;

	    original = num;

	    while (original != 0) {
	        rem = original % 10;
	        sum += rem * rem * rem;
	        original /= 10;
	    }

	    if (sum == num)
	    	 System.out.printf("%d is an Armstrong number.\n", num);
	    else
	    	 System.out.printf("%d is not an Armstrong number.\n", num);

	}

}
