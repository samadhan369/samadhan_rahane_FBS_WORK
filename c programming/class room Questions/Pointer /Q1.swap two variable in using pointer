void swap(int* ,int* );
void main()
{
	int a,b;
	printf("enter  number of a :");
	scanf("%d",&a);
	printf("enter  number of b :");
	scanf("%d",&b);
	printf("before swap a=%d,b=%d",a,b);
	swap(&a,&b);
	printf("\n");
	printf("after swap a=%d,b=%d",a,b);
	
}
void swap(int* x,int* y)
{
	int temp = *x;
	*x = *y;
	*y =temp;	
}
