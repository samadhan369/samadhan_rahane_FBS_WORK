    int voting() //def
    {
    int age;
    int* ptr = &age;
    printf("Enter the person's age: ");
    scanf("%d",ptr);
    if (*ptr >= 18)
        printf("Person is eligible to vote.\n");
    else
        printf("Person is not eligible to vote.\n");
    return 0;
}
