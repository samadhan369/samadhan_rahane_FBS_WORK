#include <stdio.h>
int lyear(); //dec
int main() {
    lyear(); // fc
}
int lyear() // def
{
    int year;
    int *ptr = &year; 
    printf("Enter a year: ");
    scanf("%d", ptr); 
    if ((*ptr % 4 == 0 && *ptr % 100 != 0) || (*ptr % 400 == 0)) {
        printf("%d is a leap year.\n", *ptr);
    } else {
        printf("%d is not a leap year.\n", *ptr);
    }
    return 0;
}
