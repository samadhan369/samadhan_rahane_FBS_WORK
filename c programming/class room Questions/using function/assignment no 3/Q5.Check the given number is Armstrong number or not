#include <stdio.h>
void armstrongn(); // dec
int main() {
    armstrongn(); //fc
}
void armstrongn()// def
{
    int num, original, rem, sum = 0;
    printf("Enter a number: ");
    scanf("%d", &num);
    original = num;
    while (original != 0) {
        rem = original % 10;
        sum += rem * rem * rem;
        original /= 10;
    }
    if (sum == num)
        printf("%d is an Armstrong number.\n", num);
    else
        printf("%d is not an Armstrong number.\n", num);
}
