// Write a C program to swap two numbers using a temporary third variable.
#include<stdio.h>
void main()
{ 
int a,b,temp;
printf("enter first number (a) :");
scanf("%d",&a);

printf("enter second number (b) :");
scanf("%d",&b);

temp=a;
a=b;
b=temp;

printf("swap number a is %d and b is %d :  ",a,b,temp);
	
}
