
#include<stdio.h>
int main()
{
	
	int hindi,marathi,c,cpp,networking,totalmarks;
	float percentage;
    printf("enter marks hindi : ");
    scanf("%d",&hindi);
    printf("enter marks marathi : ");
    scanf("%d",&marathi);
    printf("enter marks c : ");
    scanf("%d",&c);
    printf("enter marks cpp : ");
    scanf("%d",&cpp);
    printf("enter marks networking : ");
    scanf("%d",&networking);
    
    totalmarks=(hindi+marathi+c+cpp+networking);
    percentage=(totalmarks)/5.0;
    
    printf("total marks = %d\n percentage = %f\n",totalmarks,percentage);
    return 0;
}
