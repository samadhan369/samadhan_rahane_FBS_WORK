#include <stdio.h>
int factorial(int n) {
    int fact = 1, i;
    for(i = 1; i <= n; i++) {
        fact *= i;
    }
    return fact;
}
int main() {
    int num, i, temp, digit, sum;
    printf("Enter the upper limit: ");
    scanf("%d", &num);
    printf("Strong numbers between 1 and %d are:\n", num);
    for(i = 1; i <= num; i++) {
        temp = i;
        sum = 0;
        while(temp > 0) {
            digit = temp % 10;
            sum += factorial(digit);  // âœ… Corrected line
            temp /= 10;
        }
        if(sum == i) {
            printf("%d ", i);
        }
    }
    return 0;
}
