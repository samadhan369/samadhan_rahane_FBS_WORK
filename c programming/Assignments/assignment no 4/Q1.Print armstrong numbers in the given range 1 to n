#include <stdio.h>
#include <math.h>
int main() {
    int n, i, num, temp, digit, count, sum;
    printf("Enter the upper limit: ");
    scanf("%d", &n);
    printf("Armstrong numbers between 1 and %d are:\n", n);
    for (i = 1; i <= n; i++) {
        num = i;
        temp = num;
        count = 0;
        sum = 0;
        while (temp > 0) {
            count++;
            temp /= 10;
        }
        temp = num;
        while (temp > 0) {
            digit = temp % 10;
            sum += pow(digit, count);
            temp /= 10;
        }
        if (sum == num) {
            printf("%d ", num);
        }
    }
    return 0;
}
